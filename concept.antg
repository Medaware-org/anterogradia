@library "org.medaware.anterogradia.libs.ASCII" as ascii

progn {
    fun makeProgress {
        `bar := sequence {
            `[
            repeat (count = &`length, str = "=")
            `]
        }
        &`bar
    }

    sequence {
        `length := 10
        eval makeProgress

        ascii:endl()

        `length := 100
        eval makeProgress
    }
}