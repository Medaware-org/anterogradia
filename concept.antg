@library "org.medaware.anterogradia.libs.Memory"

progn {
    mem:set(key = "president",
            value = random {
                "Joe Biden",
                "Donald Trump"
            }),

    if (cond =    equal(a = mem:get(key = "president"),
                        b = "Joe Biden"),
        then = "Everything will be alright!",
        else = "We will all die!")
}